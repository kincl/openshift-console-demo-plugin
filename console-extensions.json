[
  {
    "type": "console.perspective",
    "properties": {
      "id": "demo",
      "name": "Databases",
      "icon": { "$codeRef": "perspective.icon" },
      "landingPageURL": { "$codeRef": "perspective.getLandingPageURL" },
      "importRedirectURL": { "$codeRef": "perspective.getImportRedirectURL" }
    }
  },
  {
    "type": "console.navigation/section",
    "properties": {
      "id": "demo-home",
      "perspective": "demo",
      "name": "Home"
    }
  },
  {
    "type": "console.navigation/section",
    "properties": {
      "id": "demo-workloads",
      "perspective": "demo",
      "name": "Workloads"
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "path": "/myLandingPage",
      "component": { "$codeRef": "landingPage" }
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "demo-list-databases",
      "perspective": "demo",
      "section": "demo-home",
      "name": "Dashboard",
      "href": "/myLandingPage"
    }
  },
  {
    "type": "console.navigation/resource-cluster",
    "properties": {
      "id": "mysqls",
      "perspective": "demo",
      "section": "demo-workloads",
      "name": "MySQLs",
      "model": {
        "group": "databases.example.com",
        "kind": "MariaDB",
        "version": "v1alpha1"
      }
    }
  },
  {
    "type": "console.page/resource/details",
    "properties": {
      "model": {
        "group": "databases.example.com",
        "kind": "MariaDB",
        "version": "v1alpha1"
      },
      "component": { "$codeRef": "mysqlDetails" }
    }
  }
]
